{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "daemon": false,
  "globalDependencies": [".env.default", ".env.local", "tsconfig.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {}
  }
}
